{
    "stl":{
        "pitch":3 # in nm
        "fill" : false
    },

    microscope:{
        "addressable_pixels":[65536, 56576] 
        # in 0.1us
        "max_dwt":50000 
        # horisontal screen width in nm
        "screen_width":6400 
    },

    "ordering":{
        # scanning order between slices (serpentine, or serial)
        "scanning_order" : "serpentine" 
        # order between pieces (parallel or sequential)
        "piece_order" : "parallel"
        # for faster exporting, get rid of all of the dwells which are below this cutoff time (in 0.1us)
        "cutoff_time" : 100
    },

    "geometrical":{
        "n_levels" : 20
    },

    "resistance":{
        "scale":1000
        "single_pixel_width":50 # in nm
    }
}